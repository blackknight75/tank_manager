<h1>Edit Fish MOOO</h1>

<%= form_for [@tank, @fish] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :breed %>
  <%= f.text_field :breed %>
  <%= f.collection_select :tank_id, current_user.tanks, :id, :name %>
  <%= f.label :purchase_store %>
  <%= f.text_field :purchase_store %>
  <%= f.label :purchase_date %>
  <%= f.text_field :purchase_date %>
  <%= f.submit "Save" %>
<% end %>
