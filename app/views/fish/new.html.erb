<h1>Add Fish To Tank</h1>

<%= form_for [@tank, @fish] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :breed %>
  <%= f.text_field :breed %>
  <%= f.label :purchase_store %>
  <%= f.text_field :purchase_store %>
  <%= f.label :purchase_date %>
  <%= f.text_field :purchase_date %>
  <%= f.submit "Add Fish to Tank" %>
<% end %>
